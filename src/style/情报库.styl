.m-subcont
  min-height 709px
.b-content
  box-sizing border-box
  padding 0 34px
.base
  width 100%
  border-collapse collapse
  table-layout fixed
  tr
    height 36px
    line-height 36px
    border-bottom 1px solid #dbe0e4 
    th
      font-size 14px
      color #323131
      font-weight normal
    td
      text-align center
      font-size 12px
      color #5e5e5e
      label
        vertical-align: top;
      a 
        text-decoration none
        display inline-block
        padding 0 5px
        color #4188d8
        cursor pointer
        &.delete
          color red
      &:first-child
        text-align left
        padding-left 5px
      span
        display inline-block
        height 36px
        line-height 36px
        max-width 250px
        white-space:nowrap;  
        overflow hidden
        vertical-align: top;
        text-overflow:ellipsis;

    &:last-child
      border none
//编辑
::-webkit-input-placeholder
  font-size 12px
  color #909499
.info-edit
  padding 0 40px
  ul
    padding 0
    li
      list-style none
      margin-bottom 31px
      font-size 0
      input
        outline none
        height 36px
        padding 0 8px
        border 1px solid #c1c3c4
