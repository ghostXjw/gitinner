.operate
  width 635px
  text-align right
  font-size 0
  transform translateX(75px)
  div
    display inline-block
    font-size 14px
    height 36px
    width 140px
    border-radius 3px
    text-align center
    line-height 36px
    margin-bottom 40px
    cursor pointer
  .rest
    border 1px solid #4188d8
    color #4188d8
    margin-right 20px
  .success
    background #4188d8
    color #fff
.c-url
  display inline-block
  width 80px
  height 24px
  color #fff
  font-size 12px
  background #5cc86a
  text-align center
  line-height 24px
  vertical-align moddle
  margin-left 20px
  border-radius 3px
  cursor pointer
.c-text
  background #4188d8
.start
  font-family 'MicrosoftYaHei'
  padding 0 300px 0 42px
  position relative
  .collect
    position absolute
    right 44px
    top 5px 
    width 90px
    height 30px
    line-height 30px
    border-radius 3px
    border 1px solid #d9d9d9
    text-align center
    cursor pointer
    i
      display inline-block
      width 15px
      height 12px
      background url(../image/nocollect.png)
      vertical-align: middle;
      margin-top: -3px;
      margin-right 10px
    span
      display inline-block
      vertical-align middle
      margin-top -3px
    &.active>i
      background url(../image/collect.png)

